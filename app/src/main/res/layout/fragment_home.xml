<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/enable_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingTop="@dimen/_50sdp"
        android:visibility="gone">

        <TextView
            android:id="@+id/title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/home_fragment_title"
            android:textAlignment="center"
            android:textSize="@dimen/_20sdp"
            android:textStyle="bold"
            android:textColor="@color/textColor"
            />

        <TextView
            android:id="@+id/sub_heading"
            android:layout_below="@id/title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/home_fragment_thanks"
            android:textAlignment="center"
            android:textSize="@dimen/_10sdp"
            android:textColor="@color/textColor"
            />


        <RelativeLayout
            android:layout_below="@id/sub_heading"
            android:id="@+id/layout_enable"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical"
            android:layout_marginTop="@dimen/_30sdp"
            >
            <RelativeLayout
                android:id="@+id/layout_enable_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical"
                >

            <LinearLayout
            android:layout_width="130.0dip"
            android:layout_height="130.0dip"
            android:background="@drawable/btn_background"
            android:orientation="vertical"
            android:layout_centerHorizontal="true"
            android:paddingTop="@dimen/_10sdp">

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="20.0dip"
                android:layout_height="20.0dip"
                android:layout_gravity="center"
                android:layout_marginTop="10.0dip"
                app:srcCompat="@drawable/ic_tick"
                tools:ignore="VectorDrawableCompat" />

            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="0.0sp"
                android:text="Enable"
                android:textColor="#000"
                android:textSize="20.0sp" />
        </LinearLayout>
            </RelativeLayout>

            <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/layout_enable_container"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/_60sdp"
            android:gravity="center"
            android:text="@string/app_name"
            android:textColor="#000"
            android:textSize="12.0sp"
            tools:ignore="UnknownId" />
        </RelativeLayout>


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/switch_layout"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:visibility="gone">

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:scaleType="centerCrop"
            />


        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="60.0dip"
            android:gravity="center"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="259.0dip"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/text1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="0.0dip"
                    android:text="@string/app_name"
                    android:textColor="#000"
                    android:textStyle="bold"
                    android:textSize="22.0sp" />

                <TextView
                    android:id="@+id/text2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="5.0dip"
                    android:textStyle="bold"
                    android:text="Its a easy to use app with beautiful user interface"
                    android:textAlignment="center"
                    android:textColor="#000"
                    android:textSize="13.0sp" />
            </LinearLayout>

            <ImageView
                android:layout_width="74.0dip"
                android:layout_height="73.0dip"
                android:visibility="gone"

                android:gravity="center" />
        </LinearLayout>

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="267.0dip"
            android:layout_height="130.0dip"
            android:layout_below="@+id/linearLayout2"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="30.0dip"
            android:background="@drawable/ic_main_keyboard"
            android:gravity="center" />





        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_below="@+id/imageView2"
            android:layout_marginTop="40dp"
            android:id="@+id/relativelayout"
            android:layout_height="wrap_content">



            <LinearLayout
                android:id="@+id/layout_switch"
                android:layout_width="fill_parent"
                android:layout_height="60.0dip"

                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginBottom="40.0dip"
                android:layout_marginLeft="20.0dip"
                android:layout_marginRight="20.0dip"
                android:orientation="horizontal"
                android:weightSum="5.0">

                <LinearLayout
                    android:id="@+id/layout_switch_details"
                    android:layout_width="0.0dip"
                    android:layout_height="fill_parent"
                    android:layout_weight="4.0"
                    android:background="#D3D3D3"
                    android:gravity="center"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textViewHyperLinkSettings"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="0.0dip"
                        android:text="@string/switch_to_text"
                        android:textColor="#000"
                        android:textSize="18.0sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2.0dip"
                        android:text="@string/select_from_dialog"
                        android:textColor="#000"
                        android:textSize="11.0sp"
                        android:typeface="sans" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/layout_switch_icon"
                    android:layout_width="3.0dip"
                    android:layout_height="fill_parent"
                    android:layout_alignEnd="@+id/layout_switch"
                    android:layout_alignRight="@+id/layout_switch"
                    android:layout_below="@+id/layout_switch"
                    android:layout_weight="1.0"
                    android:background="#D3D3D3"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="50.0dip"
                        android:layout_height="50.0dip"
                        custom:srcCompat="@drawable/keyboard128" />
                </LinearLayout>
            </LinearLayout>

        </RelativeLayout>

        <LinearLayout
            android:id="@+id/bottombar"
            android:layout_width="fill_parent"
            android:visibility="gone"
            android:layout_height="50.0dip"
            android:layout_alignParentBottom="true"
            android:background="@color/colorBlack"
            android:orientation="horizontal">

            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignTop="@+id/ad_banner"
                android:gravity="center_horizontal">

                <LinearLayout
                    android:id="@+id/keyboard_settings"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="10.0dip"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="15.0dip"
                        android:layout_height="15.0dip"
                        android:scaleType="fitXY"
                        custom:srcCompat="@drawable/settings128" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="Settings"
                        android:textColor="#ffffffff"
                        android:textSize="10.0sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/keyboard_themes"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_centerInParent="true"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="15.0dip"
                        android:layout_height="15.0dip"
                        android:scaleType="fitXY"
                        custom:srcCompat="@drawable/ic_action_share" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="Share"
                        android:textColor="#ffffffff"
                        android:textSize="10.0sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/more_apps"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="10.0dip"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="15.0dip"
                        android:layout_height="15.0dip"
                        android:scaleType="fitXY"
                        custom:srcCompat="@drawable/moreapps128" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="More Apps"
                        android:textColor="#ffffffff"
                        android:textSize="10.0sp" />
                </LinearLayout>


            </RelativeLayout>



        </LinearLayout>


        <!--<ImageView-->
            <!--android:id="@+id/act_keyboard_switch_iv_hand"-->
            <!--android:layout_width="60.0dip"-->
            <!--android:layout_height="60.0dip"-->
            <!--android:layout_alignBottom="@+id/relativelayout"-->
            <!--android:layout_alignParentStart="true"-->
            <!--android:layout_alignParentLeft="true"-->
            <!--android:contentDescription="@string/desc_hand"-->
            <!--android:src="@drawable/ic_hand"-->
            <!--android:visibility="gone" />-->



    </RelativeLayout>








</RelativeLayout>